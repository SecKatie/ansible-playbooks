apiVersion: v1
kind: Service
metadata:
  name: kubernetes-dashboard
  namespace: {{ k8s_dashboard_namespace }}
spec:
  type: {{ k8s_dashboard_service_type }}
  ports:
    - port: 443
      targetPort: 8443
      nodePort: {{ k8s_dashboard_node_port | int }}
      protocol: TCP
  selector:
    k8s-app: kubernetes-dashboard