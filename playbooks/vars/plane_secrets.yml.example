---
# Plane Secrets Configuration
# Copy this file to plane_secrets.yml and encrypt with ansible-vault:
#   cp plane_secrets.yml.example plane_secrets.yml
#   # Edit plane_secrets.yml with your values
#   ansible-vault encrypt plane_secrets.yml
#
# Run playbooks with: ansible-playbook ... --ask-vault-pass

# PostgreSQL password
plane_postgres_password: "change-me-to-secure-password"

# RabbitMQ password
plane_rabbitmq_password: "change-me-to-secure-password"

# Minio root password (min 8 characters)
plane_minio_root_password: "change-me-to-secure-password"

# Application secret key (generate with: openssl rand -hex 32)
plane_secret_key: "change-me-generate-with-openssl-rand-hex-32"

# Live server secret key (generate with: openssl rand -base64 32)
plane_live_server_secret_key: "change-me-generate-with-openssl-rand-base64-32"
